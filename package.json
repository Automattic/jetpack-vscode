{
  "name": "jetpack",
  "displayName": "Jetpack",
  "description": "Integrates the Jetpack toolkit into VS Code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
	"configuration":[
		{
			"title": "Analytics Settings",
			"properties": {
				"jetpackVSCode.enableAnalytics": {
					"type": "boolean",
					"default": false,
					"description": "Allow the Jetpack VSCode extension to send analytic information on command usage."
				  }
			}
		}
	],
    "commands": [
      {
        "command": "jetpack.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "jetpack.rsync",
        "title": "Jetpack: Rsync"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
